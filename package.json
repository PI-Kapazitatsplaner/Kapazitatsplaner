    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.34",
    "@types/supertest": "^2.0.12",
    "chai": "^4.3.6",
    "concurrently": "^7.2.0",
    "cross-env": "^7.0.3",
    "cypress": "^9.6.1",
    "jest": "^27.5.1",
