    "@prisma/engines": "^5.0.0",
    "config": "^3.3.7",
    "cookie-parser": "^1.4.6",
    "csurf": "^1.11.0",
    "ejs": "^3.1.8",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "keycloak-connect": "^18.0.0",
    "ssl-root-cas": "^1.3.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/config": "^0.0.41",
    "@types/cookie-parser": "^1.4.3",
    "@types/csurf": "^1.11.2",
    "@types/ejs": "^3.1.1",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
